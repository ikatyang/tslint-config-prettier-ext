{
  "name": "tslint-config-prettier-ext",
  "version": "1.0.0",
  "description": "disable all prettier-related rules",
  "keywords": [
    "prettier",
    "tslint",
    "tslint-config",
    "typescript"
  ],
  "main": "lib/rules.json",
  "repository": "https://github.com/ikatyang/tslint-config-prettier-ext",
  "homepage": "https://github.com/ikatyang/tslint-config-prettier-ext#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "yarn run build && yarn run generate",
    "lint": "tslint -p ./tsconfig.json --type-check",
    "test": "jest -c ./jest.json",
    "build": "rm -rf ./build && tsc -p ./tsconfig.build.json",
    "generate": "rm -rf ./lib && node ./build/generate.js",
    "format": "gulp format",
    "format-check": "gulp format-check",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/gulp": "3.8.33",
    "@types/jest": "20.0.5",
    "@types/make-dir": "1.0.1",
    "@types/node": "8.0.16",
    "@types/prettier": "1.5.0",
    "gulp": "3.9.1",
    "gulp-plugin-prettier": "1.0.0",
    "jest": "20.0.4",
    "make-dir": "1.0.0",
    "prettier": "1.5.3",
    "standard-version": "4.2.0",
    "ts-jest": "20.0.7",
    "ts-node": "3.3.0",
    "tslint": "5.5.0",
    "tslint-config-ikatyang": "1.0.2",
    "typescript": "2.4.2"
  },
  "files": [
    "/lib/**/*"
  ]
}
